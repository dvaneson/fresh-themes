<wx:sub-section>

  {{#> header-writing }}
  {{> header-simple _title="WRITING" _section="writing" }}
  {{/header-writing}}

  {{#each r.writing}}
  <wx:sub-section>
    <w:p wsp:rsidR="00C146CA" wsp:rsidRPr="00C146CA" wsp:rsidRDefault="00606071" wsp:rsidP="00E578D4">
      <w:pPr>
        <w:pStyle w:val="Heading2"/>
        <w:tabs>
          <w:tab w:val="right" w:pos="9360"/>
        </w:tabs>
      </w:pPr>
      {{#if url}}<w:hlink w:dest="{{{ url }}}">{{/if}}
      <w:r wsp:rsidR="009452CA" wsp:rsidRPr="00606071">
        <w:rPr>
          {{#if url}}<w:rStyle w:val="Hyperlink"/>{{/if}}
          <w:b/>
          <w:b-cs/>
        </w:rPr>
        
        <w:t>{{{ title }}}</w:t>
      </w:r>
      {{#if url}}</w:hlink>{{/if}}
      <w:r wsp:rsidR="00EA0B64">
        <w:tab/>
      </w:r>
      <w:r wsp:rsidR="00C146CA" wsp:rsidRPr="00C146CA">
        <w:rPr>
          <w:rStyle w:val="FromTo"/>
        </w:rPr>
        <w:t>{{date date 'MMM YYYY'}}</w:t>
      </w:r>
    </w:p>

    {{#if summary}}
    <w:p>
      <w:pPr>
        <w:pStyle w:val="Summary"/>
      </w:pPr>
      <w:r>
        <w:t>{{ summary }}</w:t>
      </w:r>
    </w:p>
    {{/if}}

    {{#each highlights }}
    <w:p>
      <w:pPr>
        <w:pStyle w:val="ListParagraph"/>
        <w:listPr>
          <w:ilvl w:val="0"/>
          <w:ilfo w:val="1"/>
          <wx:t wx:val="â€¢"/>
          <wx:font wx:val="Calibri"/>
        </w:listPr>
        <w:rPr>
          <w:sz-cs w:val="20"/>
        </w:rPr>
      </w:pPr>
      {{{wpml this true}}}
    </w:p>
    {{/each}}

  </wx:sub-section>

  {{/each}}

</wx:sub-section>
