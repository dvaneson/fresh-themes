<wx:sub-section>

  {{#> header-employment }}
  {{> header-simple _title="EMPLOYMENT" _section="employment" }}
  {{/header-employment}}

  {{#each r.employment.history}}
  <wx:sub-section>
    <w:p>
      <w:pPr>
        <w:pStyle w:val="Heading2" />
        <w:tabs>
          <w:tab w:val="right" w:pos="9360" />
        </w:tabs>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:b />
          <w:b-cs />
        </w:rPr>
        <w:t>{{ position }}, </w:t>
      </w:r>
      {{#if url}}<w:hlink w:dest="{{{ url }}}">{{/if}}
        <w:r>
          <w:rPr>
            {{#if url}}
            <w:rStyle w:val="Hyperlink" />{{/if}}
            <w:i />
            <w:i-cs />
          </w:rPr>
          <w:t>{{ employer }}</w:t>
        </w:r>
        {{#if url}}</w:hlink>{{/if}}
      <w:r wsp:rsidR="00EA0B64">
        <w:tab />
      </w:r>
      <w:r>
        <w:rPr>
          <w:rStyle w:val="FromTo" />
        </w:rPr>
        <w:t>{{dateRange . 'MMM YYYY'}}</w:t>
      </w:r>
    </w:p>

    {{#if summary}}
    <w:p>
      {{{wpml summary true}}}
    </w:p>
    {{/if}}

    {{#each highlights }}
    <w:p>
      <w:pPr>
        <w:pStyle w:val="ListParagraph" />
        <w:listPr>
          <w:ilvl w:val="0" />
          <w:ilfo w:val="1" />
          <wx:t wx:val="â€¢" />
          <wx:font wx:val="Calibri" />
        </w:listPr>
        <w:rPr>
          <w:sz-cs w:val="20" />
        </w:rPr>
      </w:pPr>
      {{{wpml this true}}}
    </w:p>
    {{/each}}

  </wx:sub-section>
  {{/each}}
</wx:sub-section>