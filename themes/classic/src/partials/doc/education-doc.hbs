<wx:sub-section>

  {{#> header-education }}
  {{> header-simple _title="EDUCATION" _section="education" }}
  {{/header-education}}

  {{#each r.education.history}}
  <wx:sub-section>
    <w:p>
      <w:pPr>
        <w:pStyle w:val="Heading2" />
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:b />
          <w:b-cs />
        </w:rPr>
        <w:t>{{ studyType }}</w:t>
      </w:r>
      <w:r>
        <w:t> in </w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:b />
          <w:b-cs />
        </w:rPr>
        <w:t>{{ area }}</w:t>
      </w:r>
      <w:r>
        <w:tab />
      </w:r>
      <w:r>
        <w:rPr>
          <w:rStyle w:val="FromTo" />
        </w:rPr>
        <w:t>{{dateRange . 'MMM YYYY'}}</w:t>
      </w:r>
    </w:p>

    <w:p>
      <w:pPr>
        <w:pStyle w:val="Heading2" />
        <w:tabs>
          <w:tab w:val="right" w:pos="9360" />
        </w:tabs>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:i />
          <w:i-cs />
        </w:rPr>
        <w:t>{{ institution }}</w:t>
      </w:r>
    </w:p>

    {{#if grade}}
    <w:p>
      <w:r>
        <w:rPr>
          <w:b />
          <w:b-cs />
        </w:rPr>
        <w:t>GPA: </w:t>
      </w:r>
      <w:r>
        <w:t>{{{ grade }}}</w:t>
      </w:r>
    </w:p>
    {{/if}}

    {{#each highlights}}
    <w:p>
      <w:pPr>
        <w:pStyle w:val="ListParagraph" />
        <w:listPr>
          <w:ilvl w:val="0" />
          <w:ilfo w:val="1" />
          <wx:t wx:val="â€¢" />
          <wx:font wx:val="Calibri" />
        </w:listPr>
      </w:pPr>
      <w:r>
        <w:t>{{ this }}</w:t>
      </w:r>
    </w:p>
    {{/each}}

  </wx:sub-section>
  {{/each}}
</wx:sub-section>