{{#section "skills"}}
<section id="skills">
  {{#> header-skills }}
  {{> header-icon _title="SKILLS" _section="skills" }}
  {{/header-skills}}

  <div class="template-body">
    {{#each r.skills.sets}}
    {{#> body-skills }}
    <div>
      <b>{{ name }}:</b>
      {{#each skills}}
      <span class="comma">
        {{ this }}
      </span>
      {{/each}}
    </div>
    {{/body-skills}}
    {{/each}}
  </div>
</section>
{{/section}}